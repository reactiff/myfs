<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width">
  <title>Rap Bot</title>
  <link rel="stylesheet" href="style.css">
</head>

<body>
  <h1>Rap Battle Bots</h1>


  <div id="info"></div>


  <!-- <form> -->
  <div id="bot1">

    <input id="text1" type="text" class="txt">

    <div>
      <label for="rate">Rate</label>
      <input id="rate1" type="range" min="0.5" max="2" value="1" step="0.1" >
      <div class="rate-value">1</div>
      <div class="clearfix"></div>
    </div>
    
    <div>
      <label for="pitch">Pitch</label>
      <input id="pitch" type="range" min="0" max="2" value="1" step="0.1" >
      <div class="pitch-value">1</div>
      <div class="clearfix"></div>
    </div>
   
    <div class="controls">
      <button id="button1">Play</button>

      <button id="button2">Play</button>
    </div>
  </div>



  <div id="bot1"></div>

  <script src="script.js"></script>
</body>

</html>